{
  "name": "task-api",
  "version": "1.0.0",
  "description": "",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=10.16"
  },
  "scripts": {
    "prebuild": "node ./setIndex.js && npm run clean",
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist *.tsbuildinfo",
    "prelint": "npm run prettier:fix",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test2": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "precoverage": "npm test",
    "coverage": "open coverage/index.html",
    "coverage:ci": "lb-nyc report --reporter=text-lcov | coveralls",
    "test": "lb-nyc lb-mocha --allow-console-logs \"dist/__tests__\"",
    "test:ci": "lb-nyc lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t starter .",
    "docker:run": "docker run -p 5000:5000 -d starter",
    "migrate": "node ./dist/migrate",
    "prestart": "npm run build",
    "start": "pm2 start ecosystem.config.js",
    "stop": "pm2 stop ecosystem.config.js",
    "lb4": "node node_modules/\\@loopback/cli/bin/cli-main.js",
    "lb4:app": "node node_modules/\\@loopback/cli/bin/cli-main.js app",
    "lb4:extension": "node node_modules/\\@loopback/cli/bin/cli-main.js extension",
    "lb4:controller": "node node_modules/\\@loopback/cli/bin/cli-main.js controller",
    "lb4:datasource": "node node_modules/\\@loopback/cli/bin/cli-main.js datasource",
    "lb4:model": "node node_modules/\\@loopback/cli/bin/cli-main.js model",
    "lb4:repository": "node node_modules/\\@loopback/cli/bin/cli-main.js repository",
    "lb4:service": "node node_modules/\\@loopback/cli/bin/cli-main.js service",
    "lb4:example": "node node_modules/\\@loopback/cli/bin/cli-main.js example",
    "lb4:openapi": "node node_modules/\\@loopback/cli/bin/cli-main.js openapi",
    "lb4:observer": "node node_modules/\\@loopback/cli/bin/cli-main.js observer",
    "lb4:interceptor": "node node_modules/\\@loopback/cli/bin/cli-main.js interceptor",
    "lb4:discover": "node node_modules/\\@loopback/cli/bin/cli-main.js discover",
    "lb4:relation": "node node_modules/\\@loopback/cli/bin/cli-main.js relation",
    "prepublishOnly": "npm run test",
    "restart": "pm2 kill && pm2 start ecosystem.config.js",
    "start:watch": "pm2 start ecosystem.config.js --watch",
    "dev": "tsc-watch --target es2017 --outDir ./dist --onSuccess \"node .\""
  },
  "repository": {
    "type": "git"
  },
  "author": "ITG",
  "license": "ISC",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^7.3.4",
    "@loopback/boot": "^3.4.4",
    "@loopback/context": "^3.18.0",
    "@loopback/core": "^2.18.0",
    "@loopback/extension-health": "0.4.5",
    "@loopback/extension-metrics": "0.3.5",
    "@loopback/openapi-v3": "^6.0.2",
    "@loopback/repository": "^3.7.3",
    "@loopback/rest": "^10.1.0",
    "@loopback/rest-explorer": "^3.3.4",
    "@loopback/security": "^0.5.4",
    "@loopback/service-proxy": "^3.2.4",
    "@types/jwt-decode": "2.2.1",
    "chalk": "3.0.0",
    "dotenv": "^8.2.0",
    "jwt-decode": "2.2.0",
    "loopback-connector-kv-redis": "3.0.1",
    "loopback-connector-mongodb": "4.2.0",
    "loopback-connector-oracle": "^4.5.2",
    "parse-bearer-token": "1.0.1",
    "passport-strategy": "1.0.0",
    "request": "2.88.0",
    "request-promise": "4.2.4",
    "tsc-watch": "^4.5.0"
  },
  "devDependencies": {
    "@loopback/build": "^7.0.2",
    "@loopback/cli": "^2.22.1",
    "@loopback/eslint-config": "^11.0.2",
    "@loopback/testlab": "^3.4.4",
    "@loopback/tslint-config": "2.1.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^10.17.60",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "dredd": "13.0.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-eslint-plugin": "^3.6.1",
    "eslint-plugin-mocha": "^9.0.0",
    "mocha": "^9.1.3",
    "pre-commit": "1.2.2",
    "source-map-support": "^0.5.20",
    "tslint": "5.20.0",
    "typescript": "~4.4.4"
  }
}
